// for react
{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "target": "esnext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
    ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@": [
        "./ui"
      ],
      "@/*": [
        "./ui/*"
      ],
    }
  },
  "include": [
    "ui/**/*.ts",
    "ui/**/*.tsx",
  ]
}
